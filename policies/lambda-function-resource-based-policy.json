{
"Version": "2012-10-17",
"Id": "default",
"Statement": [
    {
        "Sid": "CloudWatchEventsRuleLambdaPermission",
        "Effect": "Allow",
        "Principal": {
            "Service": "events.amazonaws.com"
        },
        "Action": "lambda:InvokeFunction",
        "Resource": "arn:aws:lambda:us-east-1:ACCOUNT_ID:function:access-analyzer-kms-function",
        "Condition": {
            "ArnLike": {
            "AWS:SourceArn": "arn:aws:events:us-east-1:ACCOUNT_ID:rule/kms-key-access-changes"
            }
        }
        }
    ]
}